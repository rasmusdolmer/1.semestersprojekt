    x:Class="_1.semesterprojekt.Forside"
    mc:Ignorable="d">

    <Grid Background="{ThemeResource ApplicationPageBackgroundThemeBrush}">
        <TextBlock HorizontalAlignment="Left" Margin="534,219,0,0" TextWrapping="Wrap" Text="Hovedmenu" VerticalAlignment="Top" Height="58" Width="852" TextAlignment="Center" FontSize="48"/>
        <Button Content="Se ordrer" HorizontalAlignment="Left" Margin="534,318,0,0" VerticalAlignment="Top" Height="168" Width="420" FontSize="28" Background="#19000000">
