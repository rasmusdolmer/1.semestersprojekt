                <Core:EventTriggerBehavior EventName="Click">
                    <Core:NavigateToPageAction TargetPage="_1.semesterprojekt.SeOrdrer"/>
                </Core:EventTriggerBehavior>
            </Interactivity:Interaction.Behaviors>
        </Button>
        <Button x:Name="BTN_Opretordre" Content="Opret ordre" HorizontalAlignment="Left" Margin="966,318,0,0" VerticalAlignment="Top" Height="168" Width="420" FontSize="28" Background="#19000000">
